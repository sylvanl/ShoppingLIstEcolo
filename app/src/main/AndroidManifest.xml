<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="demo.freakysqllite.com.shoppinglist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="demo.freakysqllite.com.shoppinglist.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="demo.freakysqllite.com.shoppinglist.InsertRowActivity" />
        <activity android:name="demo.freakysqllite.com.shoppinglist.updateRowsActivity" />
        <activity android:name="demo.freakysqllite.com.shoppinglist.deleteRowsActivity"></activity>
    </application>

</manifest>